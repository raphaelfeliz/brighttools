        "template--19577311723794__f8ba4f07-d923-4c51-96ae-71d23adfdf0e-16877746580fb4404c-1": {
          "type": "heading",
          "settings": {
            "text": "Easy Magnetic Window Cleaning Tips",
            "heading_tag": "h2",
